############################################
#       Default Configuration (anvil)      #
############################################
#
# If unspecified, the script will use the default values, deploying to the anvil local network.
#
# This is the first default private key for anvil. Nothing sensitive here.
export PRIVATE_KEY="${PRIVATE_KEY:-0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80}"

# Environment
export ENVIRONMENT=anvil_localnet

# RPC configuration
export APP_CHAIN_RPC_URL="${APP_CHAIN_RPC_URL:-http://localhost:8545}"
export PARENT_CHAIN_RPC_URL="${PARENT_CHAIN_RPC_URL:-http://localhost:8545}"

# Solidity configuration
export COMPILER_VERSION="${COMPILER_VERSION:-0.8.28}"

# Verifier configuration
export APP_CHAIN_VERIFIER="${APP_CHAIN_VERIFIER:-blockscout}"
export APP_CHAIN_VERIFIER_URL="${APP_CHAIN_VERIFIER_URL:-}"
export PARENT_CHAIN_VERIFIER="${PARENT_CHAIN_VERIFIER:-blockscout}"
export PARENT_CHAIN_VERIFIER_URL="${PARENT_CHAIN_VERIFIER_URL:-}"

### XMTP deployment configuration ###

# Admin addresses
export XMTP_GROUP_MESSAGE_BROADCASTER_ADMIN_ADDRESS="${XMTP_GROUP_MESSAGE_BROADCASTER_ADMIN_ADDRESS:-0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266}"
export XMTP_IDENTITY_UPDATE_BROADCASTER_ADMIN_ADDRESS="${XMTP_IDENTITY_UPDATE_BROADCASTER_ADMIN_ADDRESS:-0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266}"
export XMTP_NODE_REGISTRY_ADMIN_ADDRESS="${XMTP_NODE_REGISTRY_ADMIN_ADDRESS:-0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266}"
export XMTP_RATE_REGISTRY_ADMIN_ADDRESS="${XMTP_RATE_REGISTRY_ADMIN_ADDRESS:-0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266}"

# Contract addresses
export XMTP_CREATE3_FACTORY_ADDRESS="${XMTP_CREATE3_FACTORY_ADDRESS:-}"
